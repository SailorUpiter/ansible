blacklist {
 wwid .*
}

blacklist_exceptions {
 wwid {{ wwid_result.stdout }}
 
}

multipaths {
 multipath {
 wwid {{ wwid_result.stdout }}
 alias mpath0
 }
}

defaults {
 polling_interval 2
 path_selector "round-robin 0"
 path_grouping_policy multibus
 uid_attribute ID_SERIAL
 rr_min_io 100
 failback immediate
 no_path_retry queue
 user_friendly_names yes
}